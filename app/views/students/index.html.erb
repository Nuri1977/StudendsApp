<div class="row">
  <div class="col s12">
    <h3 class="center-align">Student Directory</h3>
  </div>
</div>

<div class="row">
  <% @students.each do |s| %>
    <div class="col s6 m6">
      <div class="card blue-grey darken-1">
        <div class="card-content white-text">
          <span class="card-title"><%= s.name %></span>
          <span class="card-title"><%= s.email %></span>
          <p>Corse enrollmets : None </p>
        </div>
        <div class="card-action">
          <a href="#">View Profile</a>
        </div>
      </div>
    </div>
  <% end %>
</div>